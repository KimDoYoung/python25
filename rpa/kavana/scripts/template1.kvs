INCLUDE "./examples/common.kvs" // 사용자정의함수 및  상수 설정
ENV_LOAD "./examples/env.test"  // .env 파일을 읽어서 변수로 설정

//사용자함수
FUNCTION plus(a, b)
    return a + b
END_FUNCTION

//메인함수
MAIN
    SET c = plus(1, 2)
    PRINT "Hi! kavana-script"
    
    ON_EXCEPTION //예외처리
        print ">>> {$exception_message} exit code: {$exit_code}"
    END_EXCEPTION
END_MAIN